<%= form_with url: "/applications", method: :post, data: { turbo: false } do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :street_address, "Street Address" %>
  <%= form.text_field :street_address %>
  <%= form.label :city %>
  <%= form.text_field :city %>
  <%= form.label :state %>
  <%= form.text_field :state %>
  <%= form.label :zipcode %>
  <%= form.text_field :zipcode %>
  <%= form.label :description, "Description of why I would be a good home" %>
  <%= form.text_field :description %>

  <%= form.submit "Submit" %>
<% end %>

